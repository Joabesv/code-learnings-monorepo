#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/581f518ef5401f0ee3fedab5e7b302f662efac3038b5ef5091f6d5c4c46c9e59/node_modules/turbo/bin/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/581f518ef5401f0ee3fedab5e7b302f662efac3038b5ef5091f6d5c4c46c9e59/node_modules/turbo/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/581f518ef5401f0ee3fedab5e7b302f662efac3038b5ef5091f6d5c4c46c9e59/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/node_modules:/home/joabe/.local/share/pnpm/store/v10/node_modules:/home/joabe/.local/share/pnpm/store/node_modules:/home/joabe/.local/share/pnpm/node_modules:/home/joabe/.local/share/node_modules:/home/joabe/.local/node_modules:/home/joabe/node_modules:/home/node_modules:/node_modules:/home/joabe/dev/learnings/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/581f518ef5401f0ee3fedab5e7b302f662efac3038b5ef5091f6d5c4c46c9e59/node_modules/turbo/bin/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/581f518ef5401f0ee3fedab5e7b302f662efac3038b5ef5091f6d5c4c46c9e59/node_modules/turbo/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/581f518ef5401f0ee3fedab5e7b302f662efac3038b5ef5091f6d5c4c46c9e59/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/2.5.6/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/turbo/node_modules:/home/joabe/.local/share/pnpm/store/v10/links/node_modules:/home/joabe/.local/share/pnpm/store/v10/node_modules:/home/joabe/.local/share/pnpm/store/node_modules:/home/joabe/.local/share/pnpm/node_modules:/home/joabe/.local/share/node_modules:/home/joabe/.local/node_modules:/home/joabe/node_modules:/home/node_modules:/node_modules:/home/joabe/dev/learnings/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../turbo/bin/turbo" "$@"
else
  exec node  "$basedir/../turbo/bin/turbo" "$@"
fi
